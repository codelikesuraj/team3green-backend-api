        ],
        "summary": "Publish a course (admin only)",
        "description": "Publish a course and make it available to the public",
        "responses": {
          "200": {
            "description": "Course published successfully",
            "content":{
              "application/json":{
                "schema":{
                  "type": "object",
                  "properties": {
                    "data": {
                      "type": "object",
                      "properties": {
                        "course": {
                          "$ref": "#/components/schemas/CourseItemPublished"
                        }
                      }
                    },
                    "message": {
                      "type": "string",
                      "example": "course published successfully"
                    }
                  }
                }
              }
            }
          },
          "401": {
            "$ref": "#/components/responses/UnauthenticatedError"
          },
          "403": {
            "$ref": "#/components/responses/UnauthorizedError"
          },
          "404": {
            "$ref": "#/components/responses/CourseNotFoundError"
          }
        }
      }
    },
    "/courses/{course}/unpublish": {
